{
  "name": "spotify-account-manager",
  "version": "1.0.0",
  "description": "Multi-account Spotify management application",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run client\" \"npm run server\"",
    "client": "cd client && npm start",
    "server": "cd server && npm start",
    "install-all": "npm install && cd client && npm install && cd ../server && npm install && cd .."
  },
  "keywords": [
    "spotify",
    "account-manager",
    "playlist-management"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "concurrently": "^8.2.2"
  }
}